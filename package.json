{
  "name": "sniive",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "prepare": "husky",
    "format": "prettier . --write",
    "lint-staged": "lint-staged",
    "release": "cd ./node_modules/tauri-action && pnpm i && pnpm run build && cd ../../ && node --env-file=./release.env ./node_modules/tauri-action/dist/index.js"
  },
  "lint-staged": {
    "*.{jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html,rs}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@remixicon/react": "^4.2.0",
    "@tauri-apps/api": ">=2.0.0-rc.0",
    "@tauri-apps/plugin-cli": "2.0.0-rc.1",
    "@tauri-apps/plugin-deep-link": "2.0.0-rc.1",
    "@tauri-apps/plugin-shell": "2.0.0-rc.1",
    "@tauri-apps/plugin-updater": "2.0.0-rc.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ldrs": "^1.0.2",
    "lucide-react": "^0.438.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@tauri-apps/cli": ">=2.0.0-rc.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.2",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "tauri-action": "github:tauri-apps/tauri-action",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.3.1"
  }
}
